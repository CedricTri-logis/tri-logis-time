# Local Supabase Development Environment
# Copy this file to .env.local and update with your local Supabase values
# Run `supabase start` to get these values

# API URL (default: http://localhost:54321)
SUPABASE_URL=http://localhost:54321

# Anonymous Key (get from `supabase start` output)
SUPABASE_ANON_KEY=your-local-anon-key

# Service Role Key (for admin operations - keep secret!)
SUPABASE_SERVICE_ROLE_KEY=your-local-service-role-key

# Database URL (for direct database access if needed)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# Studio URL (for browser access)
SUPABASE_STUDIO_URL=http://localhost:54323
